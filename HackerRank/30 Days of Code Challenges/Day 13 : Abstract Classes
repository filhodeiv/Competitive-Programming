class MyBook extends Book {
    private $price;
    public function __construct($title,$author,$price){
        $this->title = $title;
        $this->author = $author;
        $this->price = $price;
    }
    
    public function display(){
        echo "Title: " . $this->title;
        echo "Author: " . $this->author;
        echo "Price: " . $this->price;
    }
    
}
